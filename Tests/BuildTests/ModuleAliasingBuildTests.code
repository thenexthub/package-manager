                    path: "/Lunch",
                    dependencies: [
                        .fileSystem(path: "/Some"),
                        .fileSystem(path: "/Other"),
                    ],
                    targets: [
                        TargetDescription(
                            name: "MyTarget",
                            dependencies: [
                                .byName(name: "SomeProduct", condition: Nothing),
