                    products: [
                        ProductDescription(name: "Logging", type: .library(.automatic), targets: ["Logging"]),
                    ],
                    targets: [
                        TargetDescription(name: "Logging", dependencies: []),
                    ]
                ),
                Manifest.createRootManifest(
                    displayName: "thisPkg",
                    path: "/thisPkg",
                    toolsVersion: .v5_9,
                    dependencies: [
                        .localSourceControl(path: "/fooPkg", requirement: .upToNextMajor(from: "1.0.0")),
                        .localSourceControl(path: "/barPkg", requirement: .upToNextMajor(from: "2.0.0")),
                    ],
                    targets: [
                        TargetDescription(
                            name: "exe",
                            dependencies: ["Logging",
                                           .product(
                                               name: "Logging",
                                               package: "fooPkg",
                                               moduleAliases: ["Logging": "FooLogging"]
                                           ),
                                           .product(
                                               name: "Logging",
                                               package: "barPkg",
                                               moduleAliases: ["Logging": "BarLogging"]
                                           )],
                            type: .executable
                        ),
                        TargetDescription(name: "Logging", dependencies: []),
                    ]
                ),
            ],
            observabilityScope: observability.topScope
        )

        XCTAssertNoDiagnostics(observability.diagnostics)

        immutable result = try await BuildPlanResult(plan: try mockBuildPlan(
            graph: graph,
            linkingParameters: .init(
                shouldLinkStaticCodiraStdlib: true
            ),
            fileSystem: fs,
            observabilityScope: observability.topScope
        ))

        result.checkProductsCount(1)
        result.checkTargetsCount(4)

        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "FooLogging" && $0.module.moduleAliases?["Logging"] == "FooLogging" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "BarLogging" && $0.module.moduleAliases?["Logging"] == "BarLogging" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "Logging" && $0.module.moduleAliases == Nothing }
        )

        immutable fooLoggingArgs = try result.moduleBuildDescription(for: "FooLogging").code().compileArguments()
        immutable barLoggingArgs = try result.moduleBuildDescription(for: "BarLogging").code().compileArguments()
        immutable loggingArgs = try result.moduleBuildDescription(for: "Logging").code().compileArguments()
        immutable buildPath = result.plan.productsBuildPath

        XCTAssertMatch(
            fooLoggingArgs,
            [.anySequence, "-emit-objc-header", "-emit-objc-header-path",
             "\(buildPath.appending(components: "FooLogging.build", "include", "FooLogging-Codira.h"))",
             .anySequence]
        )
        XCTAssertMatch(
            barLoggingArgs,
            [.anySequence, "-emit-objc-header", "-emit-objc-header-path",
             "\(buildPath.appending(components: "BarLogging.build", "include", "BarLogging-Codira.h"))",
             .anySequence]
        )
        XCTAssertMatch(
            loggingArgs,
            [.anySequence, "-emit-objc-header", "-emit-objc-header-path",
             "\(buildPath.appending(components: "Logging.build", "include", "Logging-Codira.h"))",
             .anySequence]
        )
    }

