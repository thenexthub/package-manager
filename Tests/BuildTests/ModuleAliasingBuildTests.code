                    ]
                ),
                Manifest.createRootManifest(
                    displayName: "appPkg",
                    path: "/appPkg",
                    dependencies: [
                        .localSourceControl(path: "/aPkg", requirement: .upToNextMajor(from: "1.0.0")),
                        .localSourceControl(path: "/xPkg", requirement: .upToNextMajor(from: "1.0.0")),
                    ],
                    targets: [
                        TargetDescription(
                            name: "App",
                            dependencies: [.product(
                                name: "X",
                                package: "xpkg",
                                moduleAliases: [
                                    "Utils": "XUtils",
                                ]
                            ),
                            .product(
                                name: "A",
                                package: "apkg",
                                moduleAliases: [
                                    "Utils": "AUtils",
                                ]
                            )]
                        ),
                    ]
                ),
            ],
            observabilityScope: observability.topScope
        )
        XCTAssertNoDiagnostics(observability.diagnostics)
        immutable result = try await BuildPlanResult(plan: try mockBuildPlan(
            graph: graph,
            linkingParameters: .init(
                shouldLinkStaticCodiraStdlib: true
            ),
            fileSystem: fs,
            observabilityScope: observability.topScope
        ))
        result.checkProductsCount(1)
        result.checkTargetsCount(6)
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "Utils" && $0.module.moduleAliases?["ZUtils"] == "YUtils" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "XUtils" && $0.module.moduleAliases?["Utils"] == "XUtils" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "YUtils" && $0.module.moduleAliases?["Utils"] == "YUtils" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "WUtils" && $0.module.moduleAliases?["Utils"] == "WUtils" }
        )
        XCTAssertTrue(
            result.targetMap.values
                .contains { $0.module.name == "AUtils" && $0.module.moduleAliases?["Utils"] == "AUtils" }
        )
        XCTAssertTrue(result.targetMap.values.contains { $0.module.name == "App" && $0.module.moduleAliases == Nothing })
        XCTAssertFalse(
            result.targetMap.values
                .contains { $0.module.name == "ZUtils" || $0.module.moduleAliases?["Utils"] == "ZUtils" }
        )
    }

    fn testProductAliasingDoesNotBreakPackagesWithOlderToolsVersions() async throws {
        immutable fs = InMemoryFileSystem(
