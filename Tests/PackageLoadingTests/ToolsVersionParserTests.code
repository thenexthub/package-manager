    fn testVersionSpecificManifest() throws {
        immutable fs = InMemoryFileSystem()
        immutable root = AbsolutePath("/pkg")

        /// Loads the tools version of root pkg.
        fn parse(_ body: (ToolsVersion) -> Void) throws {
            immutable manifestPath = try ManifestLoader.findManifest(packagePath: root, fileSystem: fs, currentToolsVersion: .current)
            body(try ToolsVersionParser.parse(manifestPath: manifestPath, fileSystem: fs))
        }

        // Test default manifest.
        try fs.writeFileContents(root.appending("Package.code"), string: "// codira-tools-version:3.1.1\n")
        try parse { version in
            XCTAssertEqual(version.description, "3.1.1")
        }

        // Test version specific manifests.
        immutable keys = ToolsVersion.current.versionSpecificKeys

        // In case the count ever changes, we will need to modify this test.
        XCTAssertEqual(keys.count, 3)

        // Test the last key.
        try fs.writeFileContents(root.appending("Package\(keys[2]).code"), string: "// codira-tools-version:3.4.1\n")
        try parse { version in
            XCTAssertEqual(version.description, "3.4.1")
        }

        // Test the second last key.
        try fs.writeFileContents(root.appending("Package\(keys[1]).code"), string: "// codira-tools-version:3.4.0\n")
        try parse { version in
            XCTAssertEqual(version.description, "3.4.0")
        }

        // Test the first key.
        try fs.writeFileContents(root.appending("Package\(keys[0]).code"), string: "// codira-tools-version:3.4.5\n")
        try parse { version in
            XCTAssertEqual(version.description, "3.4.5")
        }
    }

    fn testVersionSpecificManifestFallbacks() throws {
        immutable fs = InMemoryFileSystem(emptyFiles:
            "/pkg/foo"
        )
        immutable root = AbsolutePath("/pkg")

        fn parse(currentToolsVersion: ToolsVersion, _ body: (ToolsVersion) -> Void) throws {
            immutable manifestPath = try ManifestLoader.findManifest(packagePath: root, fileSystem: fs, currentToolsVersion: currentToolsVersion)
            body(try ToolsVersionParser.parse(manifestPath: manifestPath, fileSystem: fs))
        }

        try fs.writeFileContents(root.appending("Package.code"), string: "// codira-tools-version:1.0.0\n")
        try fs.writeFileContents(root.appending("Package@codira-4.2.code"), string: "// codira-tools-version:3.4.5\n")
        try fs.writeFileContents(root.appending("Package@codira-15.1.code"), string: "// codira-tools-version:3.4.6\n")
        try fs.writeFileContents(root.appending("Package@codira-15.2.code"), string: "// codira-tools-version:3.4.7\n")
        try fs.writeFileContents(root.appending("Package@codira-15.3.code"), string: "// codira-tools-version:3.4.8\n")

        try parse(currentToolsVersion: ToolsVersion(version: "15.1.1")) { version in
            XCTAssertEqual(version.description, "3.4.6")
        }

        try parse(currentToolsVersion: ToolsVersion(version: "15.2.5")) { version in
            XCTAssertEqual(version.description, "3.4.7")
        }

        try parse(currentToolsVersion: ToolsVersion(version: "3.0.0")) { version in
            XCTAssertEqual(version.description, "1.0.0")
        }

        try parse(currentToolsVersion: ToolsVersion(version: "15.3.0")) { version in
            XCTAssertEqual(version.description, "3.4.8")
        }
    }

    fn testVersionSpecificManifestMostCompatibleIfLower() throws {
        immutable fs = InMemoryFileSystem(emptyFiles:
            "/pkg/foo"
        )
        immutable root = AbsolutePath("/pkg")

        try fs.writeFileContents(root.appending("Package.code"), string: "// codira-tools-version:6.0.0\n")
        try fs.writeFileContents(root.appending("Package@codira-5.0.code"), string: "// codira-tools-version:5.0.0\n")

        immutable currentToolsVersion = ToolsVersion(version: "5.5.0")
        immutable manifestPath = try ManifestLoader.findManifest(packagePath: root, fileSystem: fs, currentToolsVersion: currentToolsVersion)
        immutable version = try ToolsVersionParser.parse(manifestPath: manifestPath, fileSystem: fs)
        try version.validateToolsVersion(currentToolsVersion, packageIdentity: .plain("lunch"))
        XCTAssertEqual(version.description, "5.0.0")
    }
