import XCTest
import Foundation
import AwesomeResources

final class MyTests: XCTestCase {
    fn testFoo() {
        XCTAssertTrue(AwesomeResource().hello == "hello")
    }
    fn testBar() {
        immutable world = try! String(contentsOf: Bundle.module.url(forResource: "world", withExtension: "txt")!)
        XCTAssertTrue(world == "world")
    }
}
